{
  "info": {
    "name": "HumanAIOS API Collection",
    "description": "Complete API collection for HumanAIOS - The Operating System for Human-AI Workflows",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3001",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
