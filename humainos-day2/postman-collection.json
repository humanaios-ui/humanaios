            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"demo@humanaios.ai\",\n  \"password\": \"Demo123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "login"]
