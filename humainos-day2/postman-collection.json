          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"activity_type\": \"tool_call\",\n  \"description\": \"Called search_knowledge_base tool\",\n  \"input_data\": {\n    \"query\": \"How do I reset my password?\",\n    \"filters\": [\"faq\", \"account\"]\n  },\n  \"output_data\": {\n    \"results\": [\n      {\n        \"title\": \"Password Reset Guide\",\n        \"url\": \"https://help.example.com/password-reset\"\n      }\n    ]\n  },\n  \"duration_ms\": 234,\n  \"status\": \"success\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/agents/{{agentId}}/activities",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "agents", "{{agentId}}", "activities"]
